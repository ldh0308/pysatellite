     ...,,,--~~:::::::~~-,,....                   


       `);

class Random {
    constructor(){
	this.args = process.argv.slice(2);
    }
}
class List extends Random {
    constructor(){
	super();
    }
    print(){
	if (args.length === 0){
	    console.log('발표자가 비워있습니다.')
	} else {
	const c = Math.floor(Math.random() * this.args.length);
	console.log(`발표자 : ${this.args[c]}`);
	}
    }
}

const argv = process.argv;
const presenter = new List(argv);

presenter.print();

